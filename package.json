{
  "name": "@bitblit/ngx-obtuse",
  "version": "0.0.0-snapshot",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build-all": "ng build ngx-obtuse-common && ng build ngx-obtuse-warden",
    "set-codependencies": "echo $GITHUB_RUN_NUMBER && ./set-codependencies.sh 0.0.$GITHUB_RUN_NUMBER",
    "set-codependencies-alpha": "echo $GITHUB_RUN_NUMBER && ./set-codependencies.sh 0.0.\"$GITHUB_RUN_NUMBER\"-alpha",
    "apply-github-info": "yarn ratchet-cli apply-ci-env-variables-to-files github projects/ngx-obtuse-common/src/build/ngx-obtuse-common-info.ts projects/ngx-obtuse-warden/src/build/ngx-obtuse-warden-info.ts ",
    "publish": "mv package.json package_json.bak && pwd && ls && cd dist/ngx-obtuse-common && touch yarn.lock && pwd && ls && yarn npm publish --access public && cd ../ngx-obtuse-warden && pwd && ls && yarn npm publish --access public && cd ../.. && mv package_json.bak package.json",
    "publish-alpha": "yarn publish",
    "clean": "shx rm -Rf dist"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/router": "^18.2.0",
    "@bitblit/ratchet-common": "^4.0.469-alpha",
    "@bitblit/ratchet-graphql": "^4.0.469-alpha",
    "graphql": "16.9.0",
    "graphql-request": "7.1.0",
    "primeflex": "^3.3.1",
    "primeicons": "^7.0.0",
    "primeng": "^17.18.10",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.4",
    "@angular/cli": "^18.2.4",
    "@angular/compiler-cli": "^18.2.0",
    "@bitblit/ratchet-node-only": "4.0.469-alpha",
    "@types/jasmine": "~5.1.0",
    "cross-fetch": "4.0.0",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "luxon": "3.5.0",
    "ng-packagr": "^18.2.0",
    "shx": "0.3.4",
    "typescript": "~5.5.2"
  },
  "packageManager": "yarn@1.22.22"
}
